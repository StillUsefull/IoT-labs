{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
      "type": "object",
      "properties": {
        "longitude": { "type": "string", "pattern": "^-?[0-9]+(\\.[0-9]+)?$" },
        "latitude": { "type": "string", "pattern": "^-?[0-9]+(\\.[0-9]+)?$" }
      },
      "required": ["longitude", "latitude"]
    }
  }